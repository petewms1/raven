<h1>Songs</h1>
<table class="table table-striped">
 <thead>
  <tr>
    <th>ID</th>
        <th>
      <%= t("activerecord.attributes.song.name", :default => t("activerecord.labels.name", :default => "Name")) %>
    </th>
        <th>Created at</th>
    <th>Actions</th>
  </tr>
  </thead>
  <tbody>
  <% @songs.each do |song| -%>
   <tr>
    <td><%= song.id %></td>
        <td><%= link_to song.name, song_path(song) %>
    </td>
        <td><%= song.created_at %></td>
    <td>
      <%= link_to "Show", song_path(song) %>
      <%= link_to "Edit", edit_song_path(song) %>
      <%= link_to "Destroy", song_path(song), :method => :delete, :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}" %>
    </td>
    </tr>
    <% end -%>
   </tbody>
</table>

<%= link_to "New", new_song_path, :class => 'btn btn-primary' %>

<h1>Listing songs</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Kind</th>
    <th>Format</th>
    <th>Channels</th>
    <th>Bit rate</th>
    <th>Encoder</th>
    <th>Sample rate</th>
    <th>Date modified</th>
    <th>Length</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @songs.each do |song| %>
  <tr>
    <td><%= song.name %></td>
    <td><%= song.kind %></td>
    <td><%= song.format %></td>
    <td><%= song.channels %></td>
    <td><%= song.bit_rate %></td>
    <td><%= song.encoder %></td>
    <td><%= song.sample_rate %></td>
    <td><%= song.date_modified %></td>
    <td><%= song.length %></td>
    <td><%= link_to 'Show', song %></td>
    <td><%= link_to 'Edit', edit_song_path(song) %></td>
    <td><%= link_to 'Destroy', song, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Song', new_song_path %>
